{
    "name": "shuaijinchao/apisix-php-plugin-runner",
    "description": "Apache APISIX PHP plugin runner",
    "type": "project",
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "Jinchao Shuai",
            "email": "shuaijinchao@apache.org"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "symfony/console": "^5.3",
        "symfony/var-dumper": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "APISIX\\Commands\\": "apisix/Commands/",
            "APISIX\\Runner\\": "apisix/Runner/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "APISIX\\Tests\\": "tests/"
        }
    }
}
